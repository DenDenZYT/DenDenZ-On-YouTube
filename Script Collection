local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "DenZ", HidePremium = false, SaveConfig = false, ConfigFolder = "OrionTest"})

-- functions start

function wii()
 loadstring(game:HttpGet("https://raw.githubusercontent.com/DenDenZYT/DenDenZ-On-YouTube/main/Wii%20Hub%20Trial"))()
 wait(3)
 OrionLib:Destroy()
end

function visual()
 loadstring(game:HttpGet("https://raw.githubusercontent.com/DevTravDYT/VisualProductV10/main/KeySystem/ThankForUsing!/by-d-e-v-t-r-a-v/lua",true))()
 wait(3)
 OrionLib:Destroy()
end

function darkraiv2()
 loadstring(game:HttpGet("https://pastefy.app/y3MGGd9n/raw"))()
 wait(3)
 OrionLib:Destroy()
end

