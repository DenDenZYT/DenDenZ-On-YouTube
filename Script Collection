-- Tabs Start

local HomeTab = Window:MakeTab({
	Name = "Home",
	Icon = nil,
	PremiumOnly = false
})

local BFTab = Window:MakeTab({
	Name = "Blox Fruits",
	Icon = nil,
	PremiumOnly = false
})

local BBTab = Window:MakeTab({
	Name = "Blade Ball",
	Icon = nil,
	PremiumOnly = false
})

-- Tabs End

-- Buttons Start

HomeTab:AddButton({
	Name = "Hyper Hub",
	Callback = function()
      		hyper()
  	end    
})

