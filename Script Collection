end

function eminx()
 loadstring(game:HttpGet("https://raw.githubusercontent.com/EminenceXLua/Blade-your-Balls/main/BladeBallLoaderV2.lua"))()
 wait(3)
 setclipboard("https://discord.com/invite/a6bMcMQM5S")
 wait(1)
 OrionLib:Destroy()
end

function apple()
 _G.Ver = "1.3"
loadstring(game:HttpGet("https://raw.githubusercontent.com/ImTienNguyenZ/AppleHub/main/LoaderBF"))()
 wait(3)
 OrionLib:Destroy()
end

